
<h3><%= @question.title %></h3>
<div>
<h5><%= @question.text %></h5>
<% @question.responses.each do |response| %>
  <p><%= response %></p>
<% end %>
</div>

<div>
<% @answers.each do |answer|  %>
  <h5><%= answer %></h5>
  <% answer.responses.each do |response| %>
    <p><%= response %></p>
  <% end %>
<% end %>
</div>
